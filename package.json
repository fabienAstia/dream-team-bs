{
  "name": "dream-team-bs",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build-css": "sass --quiet --no-source-map src/scss/main.scss:src/css/dream-team-bs.css && sass --quiet --style=compressed src/css/dream-team-bs.css:dist/dream-team-bs.min.css",
    "build-js": "rollup --config",
    "start": "npm run build-js && npm run build-css"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "bootstrap": "^5.3.3",
    "rollup": "^4.24.0"
  }
}
